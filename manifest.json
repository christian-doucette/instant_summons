{
  "manifest_version": 3,
  "name": "Instant Summons",
  "version": "1.0",
  "description": "Instantly summon (upload) monster tokens by name on the Owlbear Rodeo token tab.",
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://www.owlbear.rodeo/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["data/monster_data.json"],
      "matches": ["https://www.owlbear.rodeo/*"]
    }
  ]
}
